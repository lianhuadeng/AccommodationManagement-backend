<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scu.accommodationmanagement.mapper.RepairMapper">

    <select id="getByUserId" resultType="com.scu.accommodationmanagement.model.po.Repair">
        SELECT * FROM repair WHERE student_id = #{userId} AND is_deleted = 0
    </select>
</mapper>
